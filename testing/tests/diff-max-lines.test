# Verify that when a test fails, btest-diff will not show more than the
# first TEST_DIFF_FILE_MAX_LINES lines of the file.
# %TEST-EXEC: mkdir -p Baseline/diff-max-lines
# %TEST-EXEC: echo "This is the baseline" > Baseline/diff-max-lines/child-output
# %TEST-EXEC-FAIL: btest -d %INPUT >raw 2>&1
# %TEST-EXEC: cat raw | grep -v '+++' | grep -v '\-\-\-' >output
# %TEST-EXEC: btest-diff output

@TEST-EXEC: for i in `seq 1 10`; do echo "Output line $i"; done > child-output
@TEST-EXEC: TEST_DIFF_FILE_MAX_LINES=2 btest-diff child-output
